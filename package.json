{
  "name": "redux-solid",
  "version": "0.3.4",
  "description": "A redux middleware and a SOLID (in object orient design) way to use redux. ;)",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "webpack -p --env.prod --max_old_space_size=512",
    "build:dev": "webpack -d --env.dev --max_old_space_size=512",
    "postbuild": "tsc --emitDeclarationOnly",
    "deploy": "npm run clean & npm run build & npm run postbuild & npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soorena110/redux-solid.git"
  },
  "keywords": [
    "redux",
    "solid",
    "middleware",
    "flux",
    "redux-middleware",
    "object orient design"
  ],
  "author": "MohammadReza Azarang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/soorena110/redux-solid/issues"
  },
  "homepage": "https://github.com/soorena110/redux-solid#readme",
  "devDependencies": {
    "@types/redux": "^3.6.0",
    "awesome-typescript-loader": "^5.2.1",
    "redux": "^4.0.1",
    "rimraf": "^2.6.3",
    "typescript": "^3.4.5",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  }
}
